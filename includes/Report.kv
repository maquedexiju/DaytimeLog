<ReportView>:
  navigator:navigator
  menu:menu
  reports:reports
  datePicker:date_picker
  canvas:
    Color:
      rgba:0.93,0.93,0.93,1
    Rectangle:
      size:self.size
      pos:0,0
  Navigator:
    pos:0,0
    id:navigator
  Reports:
    id:reports
    pos:0,dp(48)
    size:self.parent.width,self.parent.height-dp(182)
    size_hint:None,None
  DatePicker:
    pos:0,self.parent.height-dp(122)
    id:date_picker
  Menu:
    id:menu
    pos:self.parent.width-dp(36),self.parent.height-dp(56)
