<Navigator>:
  title:title
  today:today
  history:history
  report:report
  tab0:tab0
  tab1:tab1
  tab2:tab2
  title0:title0
  title1:title1
  title2:title2
  size:self.parent.size
  size_hint:None,None

  RelativeLayout:
    size:self.parent.width,dp(56)
    size_hint:None,None
    pos:0,self.parent.height-dp(56)
    canvas:
      Color:
        #rgba:0.97,0.97,0.97,1
        rgba:1,1,1,1
      Rectangle:
        #id:underline
        size:self.size
        pos:0,0
    Label:
      id:title
      text:'Today'
      size:self.parent.width,dp(36)
      size_hint:None,None
      text_size:self.size
      valign:"middle"
      halign:"center"
      font_size:'18sp'
      color:0,0,0,1
      pos:0,0
    RelativeLayout:
      size:self.parent.width,1
      canvas:
        Color:
          #rgba:0.18,0.18,0.18,1
          rgba:0.5,0.5,0.5,1
        Line:
          points:0,1,self.width,1
          width:1
  RelativeLayout:
    size:self.parent.width,dp(48)
    size_hint:None,None
    pos:0,0
    canvas:
      Color:
        rgba:1,1,1,1
      Rectangle:
        size:self.size
        pos:0,0
    RelativeLayout:
      size:self.parent.width/3,dp(48)
      size_hint:None,None
      pos:0,0
      ToggleButton:
        id:tab0
        size:dp(36),dp(36)
        size_hint:None,None
        background_normal:'includes/icons/Today.png'
        background_down:'includes/icons/TodayDown.png'
        border:0,0,0,0
        group:'navigator'
        center:self.parent.width/2,dp(30)
      Label:
        id:title0
        size:self.parent.width,dp(12)
        text_size:self.size
        font_size:'12sp'
        text:'Today'
        halign:'center'
        center:self.parent.width/2,dp(6)
        color:0,0,0,1
      Button:
        id:today
        size:self.parent.size
        size_hint:None,None
        #text:'Today'
        color:0,0,0,1
        background_color:0,0,0,0
        pos:0,0
        on_press:self.parent.parent.parent.ShowView('todayView')
    RelativeLayout:
      size:self.parent.width/3,dp(48)
      size_hint:None,None
      pos:self.parent.width/3,0
      ToggleButton:
        id:tab1
        size:dp(36),dp(36)
        size_hint:None,None
        background_normal:'includes/icons/History.png'
        background_down:'includes/icons/HistoryDown.png'
        border:0,0,0,0
        group:'navigator'
        center:self.parent.width/2,dp(30)
      Label:
        id:title1
        size:self.parent.width,dp(12)
        text_size:self.size
        font_size:'12sp'
        text:'History'
        halign:'center'
        center:self.parent.width/2,dp(6)
        color:0,0,0,1
      Button:
        id:history
        size:self.parent.size
        size_hint:None,None
        color:0,0,0,1
        background_color:0,0,0,0
        pos:0,0
        on_press:self.parent.parent.parent.ShowView('historyView')
    RelativeLayout:
      size:self.parent.width/3,dp(48)
      size_hint:None,None
      pos:self.parent.width/3*2,0
      ToggleButton:
        id:tab2
        size:dp(36),dp(36)
        size_hint:None,None
        background_normal:'includes/icons/Report.png'
        background_down:'includes/icons/ReportDown.png'
        border:0,0,0,0
        group:'navigator'
        center:self.parent.width/2,dp(30)
      Label:
        id:title2
        size:self.parent.width,dp(12)
        text_size:self.size
        font_size:'12sp'
        text:'Report'
        halign:'center'
        center:self.parent.width/2,dp(6)
        color:0,0,0,1
      Button:
        id:report
        size:self.parent.size
        size_hint:None,None
        color:0,0,0,1
        background_color:0,0,0,0
        pos:0,0
        on_press:self.parent.parent.parent.ShowView('reportView')
    RelativeLayout:
      size:self.parent.width,1
      canvas:
        Color:
          #rgba:0.18,0.18,0.18,1
          rgba:0.5,0.5,0.5,1
        Line:
          points:0,dp(48),self.width,dp(48)
          width:1
